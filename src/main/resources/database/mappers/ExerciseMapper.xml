<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.goody.diet.exercise.ExerciseDAO">
 
 <!-- list -->	
  	<select id="getExerciseList" parameterType="ExerciseDTO" resultType="ExerciseDTO">
		SELECT * FROM EXERCISE
	</select>

<!-- detail : urlName만 뽑아오기-->
	<select id="getExerciseBody" parameterType="ExerciseDTO" resultType="ExerciseDTO">
		SELECT URLNAME FROM EXERCISE WHERE BODYPART = #{bodyPart}
		 
	</select>
  </mapper>